mkdir VCS-lab2
cd VCS-lab2
git init
touch f1
git add .
git commit -m "Add file f1"

-> Goto github and make new repository called (VCS-lab2)

git remote add origin https://github.com/AhmedFawzyMetwaly/VSC-lab2
git push origin master 

git branch dev
git branch test

git checkout dev
touch dev-file1
touch dev-file2
git add .
git commit -m "Add files dev-file1 & dev-file2"

git push origin test
git push origin dev

-> make sure that I'm in master branch
Via git checkout master

git merge dev

// Locally
// delete local branch that is merged
    git branch -d master
//delete local branch that is unmerged
    git branch -D localBranchName

// remotely
git push origin --delete master


git tag -a v1.1 -m "my version 1.1"
git push origin v1.1
git tags

// Delete tag locally
-> git tag (TagName)
git tag -d v1.1

// Delete tag remotely
-> git push origin :tagName
-> git push --delete origin tagname

git push origin :v1.1





